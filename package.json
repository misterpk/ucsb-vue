{
  "name": "classvue1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --no-cache --open",
    "build": "run-s clean build:parcel",
    "build:parcel": "NODE_ENV=production parcel build ./src/index.html --target=browser -d ./dist --public-url ./ --no-cache",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "parcel": "^1.11.0",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "eslint": "^5.13.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "vue-template-compiler": "^2.5.21"
  }
}
